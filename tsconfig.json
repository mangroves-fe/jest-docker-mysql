{
  "compilerOptions": {
    "strictNullChecks": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "sourceMap": false,
    "allowJs": false,
    "strict": true,
    "declaration": true,
    "declarationDir": "lib",
    "rootDir": "./src",
    "outDir": "./lib",
    "target": "es6",
    "module": "CommonJS",
    "moduleResolution": "node"
  },
  "include": ["src"]
}
